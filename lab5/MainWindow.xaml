<Window x:Class="lab5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab5"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="select_sek" Content="Wybierz sekwencję" HorizontalAlignment="Left" Margin="52,63,0,0" VerticalAlignment="Top" Height="48" Width="146" Click="Button_Click"/>
        <Button x:Name="wzorce_button" Content="Znajdź wzorce" HorizontalAlignment="Left" Margin="52,130,0,0" VerticalAlignment="Top" Height="48" Width="146" Click="wzorce_button_Click"/>
        <ComboBox x:Name="wzorce_combobox" HorizontalAlignment="Left" Margin="52,195,0,0" VerticalAlignment="Top" Width="146" SelectionChanged="wzorce_combobox_SelectionChanged"/>
        <TextBox x:Name="wzorce_result" HorizontalAlignment="Left" Margin="52,272,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="143" Height="152"/>
        <TextBox x:Name="sekwencja" HorizontalAlignment="Left" Margin="318,63,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="435" Height="340"/>

    </Grid>
</Window>
